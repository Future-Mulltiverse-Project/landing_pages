{
  "extends": [
    "@nuxtjs/eslint-config-typescript",
    "plugin:prettier/recommended"
  ],
  "rules": {
    "vue/multi-word-component-names": 0,
    "vue/no-multiple-template-root": 0,
    "import/no-named-as-default": 0,
    "vue/prop-name-casing": 0,
    "vue/no-dupe-keys": 0,
    "vue/no-v-html": 0,
    "camelcase": 0
  }
}
